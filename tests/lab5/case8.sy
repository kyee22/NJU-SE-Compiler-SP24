int main(){
    int a=0;
    int b=0;
    int count=0;
    while(b<=0){
        b=b-1;
        a = 0;
        while(a<=0){
            a=a-1;
            count=count+1;
            if(a<-5)
                break;
            else
                continue;
        }
        if (b < -3)
            break;
    }

    return count;
}
